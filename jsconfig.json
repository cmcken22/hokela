{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "Actions/*": [
        "./src/actions/*"
      ],
      "Components/*": [
        "./src/components/*"
      ],
      "Pages/*": [
        "./src/pages/*"
      ],
      "Utils/*": [
        "./src/utils/*"
      ],
    }
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node-modules"
  ]
}