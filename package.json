{
  "name": "Hokela",
  "version": "1.0.0",
  "description": "Hokela web app",
  "main": "index.js",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=dev npm run watch & cross-env NODE_ENV=dev npm run watch-server",
    "start:dev-windows": "concurrently \"cross-env NODE_ENV=dev npm run watch\" \"cross-env NODE_ENV=dev npm run watch-server\"",
    "start:prod": "cross-env NODE_ENV=prod npm run webpack & cross-env NODE_ENV=prod npm run server",
    "watch-server": "nodemon server/index.js",
    "server": "node server/index.js",
    "webpack": "./node_modules/.bin/webpack",
    "watch-tests": "react-scripts test --watch --env=jsdom",
    "watch": "bash scripts/watch.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
