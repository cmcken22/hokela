    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "axios": "^0.19.0",
    "babel-loader": "^8.1.0",
    "cors": "^2.8.5",
    "css-loader": "^3.5.3",
    "ejs": "^2.6.2",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
